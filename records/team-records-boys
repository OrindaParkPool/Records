<!-- freestyle row-->

<tr>
<th align="center">Freestyle</th>

<script type="text/javascript">
for (i=1;i<=7;i++)
{
if (boyfreename[i+7]=="")//checks to be sure no tie
{
document.write("<td align=\"center\">"+ boyfreename[i]+"<br><b>"+boyfreetime[i]+"</b><br>"+boyfreeyear[i]+"<br>"+"</td>");
}
else
{
document.write("<td align=\"center\">"+ boyfreename[i]+"<br>"+boyfreeyear[i]+"<br><b>"+boyfreetime[i]+"</b><br>"+ boyfreename[i+7]+"<br>"+boyfreeyear[i+7]+"<br></td>");
}
}
</script>
</tr>

